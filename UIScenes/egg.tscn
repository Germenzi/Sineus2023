resource_name = "main"
script/source = "extends Control


func set_damage(dmg:int) -> void:
	_hide_cracks()
	if dmg != 0:
		$Egg.get_child(dmg-1).show()


func destroy() -> void:
	$Egg.hide()
	$DestroyedEgg.show()


func _hide_cracks() -> void:
	for i in $Egg.get_children():
		i.hide()
"

